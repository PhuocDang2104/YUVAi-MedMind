PROJECT_NAME=MedMind Backend
VERSION=0.1.0
API_PREFIX=/api
DATABASE_URL=postgresql+psycopg2://medmind:medmind_password@localhost:5432/medmind
REDIS_URL=redis://localhost:6379/0
CORS_ORIGINS=["http://localhost:3000","http://127.0.0.1:3000"]
SECRET_KEY=replace-with-strong-secret
ACCESS_TOKEN_EXPIRE_MINUTES=1440

# AI / LLM runtime
AI_PROVIDER=openai
AI_MODEL=gpt-4o-mini
# AI_BASE_URL=           # Optional: point to vLLM/TGI/Ollama OpenAI-compatible gateway
# AI_API_KEY=            # Required for OpenAI/Azure OpenAI (also accepts OPENAI_API_KEY)
# OPENAI_BASE_URL=       # Also accepted as alias for AI_BASE_URL
AI_REQUEST_TIMEOUT_SECONDS=30
# AI_MAX_OUTPUT_TOKENS=  # Optional: enforce token cap per response
